<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.dovar.visoc.MainActivity"
    tools:showIn="@layout/launcher_activity_main">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <com.dovar.common.vsview.DRecyclerView
            android:id="@+id/vs_list"
            android:layout_width="match_parent"
            android:layout_height="400dp">

        </com.dovar.common.vsview.DRecyclerView>

        <TextView
            android:id="@+id/tv_autoservice"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/text_margin"
            android:text="autoservice"
            />

        <FrameLayout
            android:id="@+id/fl_auto_service"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        </FrameLayout>

        <TextView
            android:id="@+id/tv_launcher"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/text_margin"
            android:text="launcher"
            />

        <TextView
            android:id="@+id/tv_xposed"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/text_margin"
            android:text="xposed"
            />

        <TextView
            android:id="@+id/tv_border_radius"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/text_margin"
            android:text="borderradius"
            />

        <FrameLayout
            android:id="@+id/fl_border_radius"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        </FrameLayout>
    </LinearLayout>

</android.support.v4.widget.NestedScrollView>
